{
    "name": "khomerikik/laravel-zero-crypto-wallet-cli",
    "description": "CLI crypto wallet developed using laravel zero",
    "keywords": [
        "laravel",
        "laravel zero",
        "console",
        "cli",
        "crypto",
        "wallet",
        "bitgo"
    ],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/KhomerikiK/laravel-zero-crypto-wallet-cli/issues",
        "source": "https://github.com/KhomerikiK/laravel-zero-crypto-wallet-cli"
    },
    "authors": [
        {
            "name": "kote khomeriki"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/database": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/view": "^9.0",
        "khomerikik/laravel-bitgo-wallet": "dev-main",
        "laminas/laminas-text": "^2.9",
        "nunomaduro/laravel-console-menu": "^3.2",
        "nunomaduro/laravel-desktop-notifier": "^2.6",
        "nunomaduro/termwind": "^1.14"
    },
    "require-dev": {
        "laravel-zero/framework": "^9.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.1",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["cwc"]
}
